msgstr ""

#: /Language/Interface/Record[Key='TutorialWireCutting']
msgctxt "Interface.TutorialWireCutting"
msgid "Wire Cutting"
msgstr ""

#: /Language/Interface/Record[Key='TutorialWireCuttingDescription']
msgctxt "Interface.TutorialWireCuttingDescription"
msgid "Welcome back Cadet, Your first task will be to clean up some old burnt out wires. Use the {THING:ItemWireCutters} from your toolbelt to remove them."
msgstr ""

#: /Language/Interface/Record[Key='TutorialWireCuttingTask1']
msgctxt "Interface.TutorialWireCuttingTask1"
