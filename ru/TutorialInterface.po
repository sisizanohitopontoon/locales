msgstr ""

#: /Language/Interface/Record[Key='SettingUpThePowerControllerTask4']
msgctxt "Interface.SettingUpThePowerControllerTask4"
msgid "Place the {THING:ItemBatteryCellLarge} in to the {THING:StructureAreaPowerControl}."
msgstr "Поместите {THING:ItemBatteryCellLarge} в {THING:StructureAreaPowerControl}."

#: /Language/Interface/Record[Key='SettingUpThePowerControllerTask5']
msgctxt "Interface.SettingUpThePowerControllerTask5"
msgid "Flip the breaker to the “On” position on the {THING:StructureAreaPowerControl}."
msgstr ""

