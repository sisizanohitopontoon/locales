msgstr "{KEY:HelmetSlot}-{KEY:ToolBeltSlot}"

#: /Language/Interface/Record[Key='OpenBackControlsDisc']
msgctxt "Interface.OpenBackControlsDisc"
msgid "Open equipped slots"
msgstr "打开装备插槽"

#: /Language/Interface/Record[Key='GettingTheBelt']
msgctxt "Interface.GettingTheBelt"
