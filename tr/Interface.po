msgstr "Cihaz atmosferindeki azot oksit oranı"

#: /Language/Interface/Record[Key='LogicTypeRatioWater']
msgctxt "Interface.LogicTypeRatioWater"
msgid "The ratio of water in device atmosphere"
msgstr "Cihaz atmosferindeki su oranı"

#: /Language/Interface/Record[Key='LogicTypeRatioVolatiles']
msgctxt "Interface.LogicTypeRatioVolatiles"
msgid "The ratio of volatiles in device atmosphere"
msgstr "Cihaz atmosferindeki uçucu madde oranı"

#: /Language/Interface/Record[Key='LogicTypeRatioPollutant']
msgctxt "Interface.LogicTypeRatioPollutant"
msgid "The ratio of pollutant in device atmosphere"
msgstr "Cihaz atmosferindeki kirletici madde oranı"

#: /Language/Interface/Record[Key='LogicTypeRatioNitrogen']
msgctxt "Interface.LogicTypeRatioNitrogen"
msgid "The ratio of nitrogen in device atmosphere"
msgstr "Cihaz atmosferindeki azot oranı"

#: /Language/Interface/Record[Key='LogicTypeRatioCarbonDioxide']
msgctxt "Interface.LogicTypeRatioCarbonDioxide"
msgid "The ratio of co2 in device atmosphere"
msgstr "Cihaz atmosferindeki CO2 oranı"

#: /Language/Interface/Record[Key='LogicTypeRatioOxygen']
msgctxt "Interface.LogicTypeRatioOxygen"
