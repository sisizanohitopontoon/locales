msgstr "套件 旋轉警示燈"

#: /Language/Things/RecordThing[Key='ItemFireExtinguisher']
msgctxt "Things.ItemFireExtinguisher"
msgid "{BLANK}"
msgstr "{BLANK}"

#: /Language/Things/RecordThing[Key='ItemFireExtinguisher']
msgctxt "Things.ItemFireExtinguisher"
msgid "Fire Extinguisher"
msgstr "滅火器"

#: /Language/Things/RecordThing[Key='ItemFertilizedEgg']
msgctxt "Things.ItemFertilizedEgg"
msgid "{BLANK}"
